    "def count_words(sentence):\n",
    "    words = sentence.split()\n",
    "    return len(words)\n",
    "\n",
    "# Test the function\n",
    "input_str = \"The quick brown fox jumps over the lazy dog\"\n",
    "result = count_words(input_str)\n",
    "print(\"Number of words:\", result)  # Output: 9\n"

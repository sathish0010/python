   "source": [
    "# Python version of digits_count\n",
    "n = int(input(\"Enter a number: \"))\n",
    "sum = 0\n",
    "\n",
    "while n > 0:\n",
    "    digit = n % 10\n",
    "    sum += digit\n",
    "    n //= 10\n",
    "\n",
    "print(sum)\n"
   ]
  }
